{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "start:dev": "npm run dev",
    "start:services": "npm run dev",
    "start:verified": "powershell -ExecutionPolicy Bypass -File ./scripts/start-and-verify.ps1",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:production": "vite build && esbuild server/index-production.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --outfile=dist/index-production.js",
    "build:azure": "vite build && esbuild server/index-azure.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "start:production": "cross-env NODE_ENV=development tsx server/index-production.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest --run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "verify": "node scripts/verify-changes.js",
    "verify:full": "npm run verify && npm run lint && npm run check",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "audit:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:watch": "nodemon --watch server --ext ts --exec \"npm run docs:generate\"",
    "deploy:staging": "node scripts/deploy.js --env=staging",
    "deploy:production": "node scripts/deploy.js --env=production",
    "cache:verify": "powershell -ExecutionPolicy Bypass -File ./scripts/verify-cache-headers.ps1",
    "cache:clear": "powershell -Command \"Write-Host 'Cache clearing: Restart dev server to clear browser cache. Use Ctrl+Shift+R in browser for hard refresh.' -ForegroundColor Yellow\""
  },
